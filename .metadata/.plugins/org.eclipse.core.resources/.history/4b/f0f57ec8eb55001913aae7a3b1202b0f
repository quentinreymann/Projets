package com.adaming.bean;

public class Country {
	
	public String nom;
	
		
	public Country() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	public Country(String nom) {
		super();
		this.nom = nom;


	}
	public String getNom() {
		return nom;
	}
	public void setNom(String nom) {
		this.nom = nom;
	}
	
}
