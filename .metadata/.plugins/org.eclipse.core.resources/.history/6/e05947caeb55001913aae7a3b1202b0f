package com.adaming.bean;

import java.util.ArrayList;
import java.util.List;

public class CountryService {
	
	public static List<String> createCountry(String string) {
		List<String> list = new ArrayList<String>();
		list.add(string);
		return list;
	}
	public static void main(String[] args){
	createCountry("France");
	createCountry("Egypte");
	createCountry("USA");
	createCountry("Japon");
	createCountry("Australie");
	}
}
